pipeline {
  agent any {
    Stages {
      stage("Build_now") {
        steps {
          echo "Building.."
        }
      }
      stage("Test_now") {
        steps {
          echo "Testing.."
        }
      }
      stage("Deploy_now") {
        steps {
          echo "Deploying.."  
        }
      }
    }
  }
}
